<!ELEMENT SPLITS_XML (JORNADAS+)>
<!ELEMENT JORNADAS (JORNADAS_XML+)>
<!ELEMENT JORNADAS_XML (PARTIDOS+)>
<!ELEMENT PARTIDOS (PARTIDOS_XML+)>
<!ELEMENT PARTIDOS_XML ( HORA, EQUIPO_LOCAL, EQUIPO_VISITANTE)>
<!ELEMENT HORA (#PCDATA)>
